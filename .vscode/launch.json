            "request": "launch",
            "preLaunchTask": "build-API.DojoSurveysApi",
            "program": "${workspaceFolder}\\src\\+Modules\\Required\\DojoSurveyModule\\+Api\\API.DojoSurveysApi\\src\\bin\\Debug\\net7.0\\API.DojoSurveysApi.dll",
